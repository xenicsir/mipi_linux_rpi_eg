#!/bin/bash

# Directories
CURRENT_PWD=$(pwd)
LINUX_RPI=${CURRENT_PWD}
LINUX_RPI_SRC=${LINUX_RPI}/kernel
LINUX_RPI_INSTALL=${LINUX_RPI}/sources/linux_install
LINUX_RPI_INSTALL_KERNEL=${LINUX_RPI_INSTALL}/kernel
LINUX_RPI_INSTALL_MODULES=${LINUX_RPI_INSTALL}

KERN_ARCH=arm64
KERN_CROSS_COMPILE=aarch64-linux-gnu-
KERN_KERNEL=kernel8
KERN_LOCALVERSION=-eg

